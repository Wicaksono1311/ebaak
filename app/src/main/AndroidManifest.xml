<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dennyprastiawan.ebaak">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Ebaak">
        <activity android:name=".desain.Yudisium"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".desain.Tandalulus"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Pindahprodi"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Pindahkelas"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Kerjapraktek"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Penelitian"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Aktifakademik"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Cutiakademik"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.SuratSidang"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Suratketerangan"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".desain.Daftar"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".desain.Login"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".desain.Splashactivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>